{
	"type" : "sequential",
	"config" : {
		"item" : {
			"data" : {
				"size" : "10MB"
			},
			"output" : {
				"path" : "read-fixed-ranges"
			}
		},
		"load" : {
			"limit" : {
				"count" : 100
			}
		}
	},
	"jobs": [
