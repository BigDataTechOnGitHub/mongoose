					}
				}
			}
		}, {
			"type": "load",
			"config" : {
				"item" : {
					"data" : {
						"ranges" : {
							"fixed" : [
								"4MB-5MB"
							]
						}
					},
					"input" : {
						"file" : "read-fixed-ranges.csv"
					}
				},
				"load" : {
					"type" : "read"
				}
			}
		}
	]
