{
	"type" : "sequential",
	"config" : {
		"item" : {
			"data" : {
				"size" : "4KB"
			}
		},
		"load" : {
			"limit" : {
				"count" : 10000
			},
			"concurrency" : 4
		},
		"storage" : {
			"type" : "fs"
		}
	},
	"jobs" : [
		{
			"type" : "precondition",
			"config" : {
				"item" : {
					"output" : {
						"path" : "/tmp/read-files-from-variable-path/%p{16;2}",
						"file" : "read-files-from-variable-path.csv"
					},
					"data" : {
						"size" : "4KB"
					}
				}
			}
		}, {
			"type" : "load",
			"config" : {
				"item" : {
					"input" : {
						"file" : "read-files-from-variable-path.csv"
					}
				},
				"load" : {
					"type" : "read"
				}
			}
		}
	]
}
