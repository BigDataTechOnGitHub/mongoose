{
	"type" : "sequential",
	"config" : {
		"item" : {
			"data" : {
				"size" : "16KB"
			}
		},
		"load" : {
			"concurrency" : 10
		}
	},
	"jobs" : [
		{
			"type" : "precondition",
			"config" : {
				"item" : {
					"output" : {
						"file" : "circular-append-before.csv"
					}
				},
				"load" : {
					"limit" : {
						"count" : 1000
					}
				}
			}
		},
		{
			"type" : "load",
			"config" : {
				"item" : {
					"data" : {
						"ranges" : {
							"fixed" : "-4KB"
						}
					},
					"input" : {
						"file" : "circular-append-before.csv"
					},
					"output" : {
						"file" : "circular-append-after.csv"
					}
				},
				"load" : {
					"type" : "update",
					"circular" : true,
					"limit" : {
						"count" : 31000
					}
				}
			}
		}
	]
}
