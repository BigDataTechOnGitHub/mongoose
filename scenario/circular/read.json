{
	"type" : "sequential",
	"config" : {
		"item" : {
			"output" : {
				"path" : "circular-read"
			}
		},
		"load" : {
			"limit" : {
				"count" : 1
			}
		}
	},
	"jobs": [
		{
			"type": "precondition",
			"config" : {
				"item" : {
					"output" : {
						"file" : "circular-read.csv"
					}
				}
			}
		}, {
			"type": "load",
			"config" : {
				"item" : {
					"input" : {
						"file" : "circular-read.csv"
					}
				},
				"load" : {
					"type" : "read",
					"circular" : true,
					"limit" : {
						// count -> infinity instead of inherited value of 1, time -> 1 minute
						"count" : 0,
						"time" : "1m"
					}
				}
			}
		}
	]
}
