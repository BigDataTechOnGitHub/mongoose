{
	"type" : "sequential",
	"config" : {
		"item" : {
			"output" : {
				"path" : "circular-read"
			}
		}
	},
	"jobs": [
		{
			"type": "precondition",
			"config" : {
				"item" : {
					"output" : {
						"file" : "circular-read.csv"
					}
				},
				"load" : {
					"limit" : {
						"count" : 1
					}
				}
			}
		}, {
			"type": "load",
			"config" : {
				"item" : {
					"input" : {
						"file" : "circular-read.csv"
					}
				},
				"load" : {
					"type" : "read",
					"circular" : true,
					"limit" : {
						"time" : "1m"
					}
				}
			}
		}
	]
}
