{
	"type" : "sequential",
	"jobs": [
		{
			"type": "precondition",
			"config" : {
				"item" : {
					"output" : {
						"file" : "circular-read.csv"
					}
				},
				"test" : {
					"step" : {
						"limit" : {
							"count" : 1
						}
					}
				}
			}
		}, {
			"type": "load",
			"config" : {
				"item" : {
					"input" : {
						"file" : "circular-read.csv"
					}
				},
				"load" : {
					"type" : "read",
					"circular" : true
				},
				"test" : {
					"step" : {
						"limit" : {
							"time" : "1m"
						}
					}
				}
			}
		}
	]
}
