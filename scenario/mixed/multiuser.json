{
	"type" : "sequential",
	"config" : {
		"storage" : {
			"driver" : {
				"concurrency" : 10,
				"type" : "s3"
			}
		}
	},
	"jobs" : [
		{
			"type" : "precondition",
			"config" : {
				"item" : {
					"naming" : {
						"length" : 9,
						"offset" : 0,
						"prefix" : "bucket-",
						"radix" : 10,
						"type" : "asc"
					},
					"type" : "path"
				},
				"test" : {
					"step" : {
						"limit" : {
							"count" : 100
						}
					}
				}
			}
		},
		{
			"type" : "load",
			"config" : {
				"item" : {
					"data" : {
						"size" : "10KB"
					},
					"output" : {
						"file" : "objects.csv",
						"path" : "bucket-%d{00}[0-99]"
					}
				},
				"storage" : {
					"auth" : {
						"file" : "credentials.csv",
						"uid" : "user-%d{00}[0-99]"
					}
				},
				"test" : {
					"step" : {
						"limit" : {
							"count" : 10000
						}
					}
				}
			}
		}
	]
}
