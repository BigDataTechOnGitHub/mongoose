{
	"type" : "sequential",
	"config" : {
		"item" : {
			"output" : {
				"path" : "/weighted-load"
			}
		},
		"load" : {
			"concurrency" : 100
		}
	},
	"jobs" : [
		{
			"type" : "precondition",
			"config" : {
				"item" : {
					"data" : {
						"size" : "1KB"
					},
					"output" : {
						"file" : "weighted-load.csv"
					}
				},
				"load" : {
					"limit" : {
						"count" : 10000
					},
					"metrics" : {
						"period" : 0
					}
				}
			}
		}, {
			"type" : "mixed",
			"weights" : [
				20,
				80
			],
			"config" : [
				{
					"item" : {
						"data" : {
							"size" : "4KB-16KB"
						}
					}
				},
				{
					"item" : {
						"input" : {
							"file" : "weighted-load.csv"
						}
					},
					"load" : {
						"circular" : true,
						"limit" : {
							"time" : "5m"
						},
						"type" : "read"
					}
				}
			]
		}
	]
}
