{
	"type" : "sequential",
	"config" : {
		"storage" : {
			"net" : {
				"http" : {
					"api" : "s3"
				},
				"node" : {
					"port" : 9020
				}
			}
		}
	},
	"jobs" : [
		{
			"type" : "precondition",
			"config" : {
				"item" : {
					"data" : {
						"size" : "10KB"
					},
					"output" : {
						"file" : "${MONGOOSE_DIR}/log/read-10k-list.csv"
					}
				},
				"load" : {
					"limit" : {
						"count" : 50000
					},
					"concurrency" : 100
				}
			}
		},
		{
			"type" : "command",
			"value" : "sleep ${WAIT_TIME}",
			"blocking" : true
		},
		{
			"type" : "mixed",
			"weights" : [
				10,
				90
			],
			"config" : [
				{
					"item" : {
						"data" : {
							"size" : "10KB"
						},
						"output" : {
							"file" : "${MONGOOSE_DIR}/log/MAX-W10KB/items.csv"
						}
					},
					"load" : {
						"limit" : {
							"time" : "${RUN_TIME}"
						},
						"job" : {
							"name" : "MAX-W10KB"
						},
						"concurrency" : 2000
					}
				},
				{
					"item" : {
						"input" : {
							"file" : "${MONGOOSE_DIR}/log/read-10k-list.csv"
						}
					},
					"load" : {
						"type" : "read",
						"circular" : true
					}
				}
			]
		}
	]
}
