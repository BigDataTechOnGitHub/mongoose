{
	"$schema" : "http://json-schema.org/draft-04/schema#",
	"definitions" : {
		"config" : {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"item": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"data": {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"content": {
									"type": "object",
									"additionalProperties": false,
									"properties": {
										"file": {
											"type": [
												"null", "string"
											]
										},
										"seed": {
											"type": "string",
											"pattern" : "[A-Fa-f0-9]+"
										},
										"ringSize": {
											"oneOf" : [
												{
													"type": "integer",
													"minimum" : 1
												},
												{
													"type" : "string",
													"pattern" : "[\\d\\.]+[KMGTPE]?B?"
												}
											]
										}
									}
								},
								"ranges": {
									"type" : "object",
									"additionalProperties" : false,
									"properties" : {
										"fixed" : {
											"oneOf" : [
												{
													"type" : "null"
												},
												{
													"type" : "string",
													"pattern" : "[\\d\\.]+[KMGTPE]?B?\\-?[\\d\\.]?[KMGTPE]?B?"
												}
											]
										},
										"random" : {
											"type" : "number",
											"minimum" : 0
										},
										"threshold" : {
											"oneOf" : [
												{
													"type" : "integer",
													"minimum" : 0
												},
												{
													"type" : "string",
													"pattern" : "[\\d\\.]+[KMGTPE]?B?"
												}
											]
										}
									}
								},
								"size": {
									"oneOf" : [
										{
											"type": "integer",
											"minimum" : 0
										}, {
											"type" : "string",
											"pattern" : "[\\d\\.]+[KMGTPE]?B?"
										}
									]
								},
								"verify": {
									"type": "boolean"
								}
							}
						},
						"input": {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"file": {
									"type": [
										"null", "string"
									]
								},
								"path": {
									"type": [
										"null", "string"
									]
								}
							}
						},
						"naming": {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"type": {
									"type": "string",
									"enum" : [
										"asc", "desc", "random"
									]
								},
								"prefix": {
									"type": [
										"null", "string"
									]
								},
								"radix": {
									"type": "integer",
									"minimum" : 2,
									"maximum" : 36
								},
								"offset": {
									"type": "integer",
									"minimum" : 0
								},
								"length": {
									"type": "integer",
									"minimum" : 1
								}
