						"reuseAddr": {
							"type": "boolean"
						},
						"sndBuf" : {
							"oneOf" : [
								{
									"type" : "integer",
									"minimum" : 0
								},
								{
									"type" : "string",
									"pattern" : "[\\d\\.]+[KMGTPE]?B?"
								}
							]
						},
						"tcpNoDelay": {
							"type": "boolean"
						},
						"timeoutMilliSec": {
							"type": "integer",
							"minimum" : 0
						}
					}
				},
				"storage": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"auth": {
							"type": "object",
