						"driver" : {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"addrs": {
									"type": "array",
									"items": {
										"type" : "string"
									}
								},
								"port" : {
									"type" : "number",
									"minimum" : 1
								},
								"remote" : {
									"type" : "boolean"
								}
							}
						},
						"http": {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"api": {
									"type": "string",
									"pattern" : "[\\w\\d]+"
								},
								"fsAccess": {
									"type": "boolean"
								},
								"headers": {
									"type": "object",
									"properties": {
									}
								},
								"namespace": {
									"type": [
										"null",
										"string"
									]
								},
								"versioning": {
									"type": "boolean"
								}
							}
						},
						"mock": {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"capacity": {
									"type": "integer",
