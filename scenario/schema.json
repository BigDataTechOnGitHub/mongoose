						},
						"output": {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"file": {
									"type": [
										"null", "string"
									]
								},
								"path": {
									"type": [
										"null", "string"
									]
								}
							}
						},
						"type" : {
							"enum" : [
								"data", "path", "token"
							]
