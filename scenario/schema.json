										"connections" : {
											"type" : "integer",
											"minimum" : 0
										},
										"responses" : {
											"type" : "integer",
											"minimum" : 0
										}
									}
								},
								"headCount": {
									"type": "integer",
									"minimum" : 1
								},
								"node" : {
									"type" : "boolean"
								}
