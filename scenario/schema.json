	},
	"type" : "object",
	"additionalProperties" : false,
	"properties" : {
		"blocking" : {
			"type" : "boolean"
		},
		"config" : {
			"oneOf" : [
				{
					"$ref" : "#/definitions/config"
				},
				{
					"type" : "array",
					"items" : {
						"$ref" : "#/definitions/config"
					}
				}
			]
		},
		"in" : {
			"oneOf" : [
				{
					"type" : "array"
				},
				{
					"type" : "string"
