						},
						"node" : {
							"type" : "object",
							"additionalProperties" : false,
							"properties" : {
								"addrs": {
									"type": "array",
									"items": {
										"type" : "string"
									}
								},
								"port" : {
									"type" : "number",
									"minimum" : 1
								}
							}
						},
						"ssl" : {
							"type" : "boolean"
						},
						"type": {
							"type": "string",
							"enum" : [
								"fs", "http"
							]
						}
					}
				},
				"version" : {
					"type" : "string"
				}
			}
