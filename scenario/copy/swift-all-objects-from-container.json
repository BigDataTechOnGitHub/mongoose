{
	"type" : "sequential",
	"config" : {
		"item" : {
			"data" : {
				"size" : "1KB"
			},
			"output" : {
				"path" : "copy-swift-object"
			}
		},
		"load" : {
			"limit" : {
				"count" : 1000
			},
			"concurrency" : 10
		},
		"storage" : {
			"driver" : {
				"type" : "swift"
			},
			"net" : {
				"http" : {
					"namespace" : "examplens"
				}
			}
		}
	},
	"jobs" : [
		{
			"type" : "precondition"
		}, {
			"type" : "load",
			"config" : {
				"item" : {
					"input" : {
						"path" : "copy-swift-object"
					},
					"output" : {
						"path" : "copy-swift-object_"
					}
				}
			}
		}
	]
}
