{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"additionalProperties": false,
	"properties": {
		"aliasing" : {
			"type" : "array",
			"uniqueItems" : true,
			"items" : {
				"type" : "object",
				"properties": {
					"name" : {
						"type" : "string"
					},
					"deprecation" : {
						"type" : "boolean"
					},
					"target" : {
						"type" : "string"
					}
				},
				"required" : [
					"name", "target"
				]
			}
		},
		"item": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"data": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"content": {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"file": {
									"type": [
										"null", "string"
									]
								},
								"seed": {
									"type": "string",
									"pattern" : "[A-Fa-f0-9]+"
								},
								"ringSize": {
									"oneOf" : [
										{
											"type": "integer",
											"minimum" : 1
										},
										{
											"type" : "string",
											"pattern" : "[\\d\\.]+[KMGTPE]?B?"
										}
									]
								}
							},
							"required": [
								"file", "seed", "ringSize"
							]
						},
						"ranges": {
							"type" : "object",
							"additionalProperties" : false,
							"properties" : {
								"fixed" : {
									"oneOf" : [
										{
											"type" : "null"
										},
										{
											"type" : "string",
											"pattern" : "[\\d\\.]+[KMGTPE]?B?\\-?[\\d\\.]?[KMGTPE]?B?"
										}
									]
								},
								"random" : {
									"type" : "number",
									"minimum" : 0
								},
								"threshold" : {
									"oneOf" : [
										{
											"type" : "integer",
											"minimum" : 0
										},
										{
											"type" : "string",
											"pattern" : "[\\d\\.]+[KMGTPE]?B?"
										}
									]
								}
							},
							"required" : [
								"fixed", "random", "threshold"
							]
						},
						"size": {
							"oneOf" : [
								{
									"type": "integer",
									"minimum" : 0
								}, {
									"type" : "string",
									"pattern" : "[\\d\\.]+[KMGTPE]?B?"
								}
							]
						},
						"verify": {
							"type": "boolean"
						}
					},
					"required": [
						"content", "ranges", "size", "verify"
					]
				},
				"input": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"file": {
							"type": [
								"null", "string"
							]
						},
						"path": {
							"type": [
								"null", "string"
							]
						}
					},
					"required": [
						"file", "path"
					]
				},
				"naming": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"type": {
							"type": "string",
							"enum" : [
								"asc", "desc", "random"
							]
						},
						"prefix": {
							"type": [
								"null", "string"
							]
						},
						"radix": {
							"type": "integer",
							"minimum" : 2,
							"maximum" : 36
						},
						"offset": {
							"type": "integer",
							"minimum" : 0
						},
						"length": {
							"type": "integer",
							"minimum" : 1
						}
					},
					"required": [
						"type", "prefix", "radix", "offset", "length"
					]
				},
				"output": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"delay" : {
							"oneOf" : [
								{
									"type" : "integer",
									"minimum" : 0
								},
								{
									"type" : "string",
									"pattern" : "[\\d]+[SsMmHhDd]?"
								}
							]
						},
						"file": {
							"type": [
								"null", "string"
							]
						},
						"path": {
							"type": [
								"null", "string"
							]
						}
					},
					"required": [
						"delay", "file", "path"
					]
				},
				"type" : {
					"enum" : [
						"data", "path", "token"
					]
				}
			},
			"required": [
				"data", "input", "naming", "output", "type"
			]
		},
		"load": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"circular": {
					"type": "boolean"
				},
				"concurrency": {
					"type": "integer",
					"minimum" : 1
				},
				"generator" : {
					"type" : "object",
					"additionalProperties" : false,
					"properties" : {
						"addrs": {
							"type": "array",
							"items": {
								"type" : "string"
							}
						},
						"remote" : {
							"type" : "boolean"
						},
						"shuffle" : {
							"type" : "boolean"
						}
					},
					"required" : [
						"addrs", "remote", "shuffle"
					]
				},
				"job" : {
					"type" : "object",
					"additionalProperties" : false,
					"properties" : {
						"name" : {
							"type" : [
								"null", "string"
							]
						}
					},
					"required" : [
						"name"
					]
				},
				"limit": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"count": {
							"type": "integer",
							"minimum" : 0
						},
						"rate": {
							"type": "number",
							"minimum" : 0
						},
						"size": {
							"oneOf" : [
								{
									"type": "integer",
									"minimum" : 0
								},
								{
									"type" : "string",
									"pattern" : "[\\d\\.]+[KMGTPE]?B?\\-?[\\d\\.]?[KMGTPE]?B?"
								}
							]
						},
						"time": {
							"oneOf" : [
								{
									"type" : "integer",
									"minimum" : 0
								},
								{
									"type" : "string",
									"pattern" : "[\\d]+[SsMmHhDd]?"
								}
							]
						}
					},
					"required": [
						"count", "rate", "size", "time"
					]
				},
				"metrics" : {
					"type" : "object",
					"additionalProperties" : false,
					"properties" : {
						"period" : {
							"oneOf" : [
								{
									"type" : "integer",
									"minimum" : 0
								}, {
									"type" : "string",
									"pattern" : "[\\d]+[SsMmHhDd]?"
								}
							]
						},
						"precondition" : {
							"type" : "boolean"
						},
						"threshold" : {
							"type" : "number",
							"minimum" : 0,
							"maximum" : 1
						},
						"trace" : {
							"type" : "object",
							"additionalProperties" : false,
							"properties" : {
								"storageDriver" : {
									"type" : "boolean"
								},
								"storageNode" : {
									"type" : "boolean"
								},
								"itemInfo" : {
									"type" : "boolean"
								},
								"ioTypeCode" : {
									"type" : "boolean"
								},
								"statusCode" : {
									"type" : "boolean"
								},
								"reqTimeStart" : {
									"type" : "boolean"
								},
								"duration" : {
									"type" : "boolean"
								},
								"respLatency" : {
									"type" : "boolean"
								},
								"dataLatency" : {
									"type" : "boolean"
								},
								"transferSize" : {
									"type" : "boolean"
								}
							},
							"required" : [
								"storageDriver", "storageNode", "itemInfo", "ioTypeCode",
								"statusCode", "reqTimeStart", "duration", "respLatency",
								"dataLatency", "transferSize"
							]
						}
					},
					"required" : [
						"period", "precondition", "threshold", "trace"
					]
				},
				"queue" : {
					"type" : "object",
					"additionalProperties" : false,
					"properties" : {
						"size" : {
							"type" : "integer",
							"minimum" : 1
						}
					},
					"required" : [
						"size"
					]
				},
				"type": {
					"type": "string",
					"pattern" : "[NnOoPpCcRrEeAaTtDdUuLl]+=?[\\d]?%?",
					"minLength" : 4,
					"maxLength" : 6
				}
			},
			"required": [
				"circular", "concurrency", "generator", "job", "limit", "metrics", "type"
			]
		},
		"scenario" : {
			"type" : "object",
			"additionalProperties" : false,
			"properties" : {
				"file" : {
					"type" : [
						"null", "string"
					]
				}
			},
			"required" : [
				"file"
			]
		},
		"storage": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
				"auth": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"id": {
							"type": [
								"null", "string"
							]
						},
						"secret": {
							"type": [
								"null", "string"
							]
						},
						"token": {
							"type": [
								"null", "string"
							]
						}
					},
					"required": [
						"id", "secret", "token"
					]
				},
				"driver" : {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"addrs": {
							"type": "array",
							"items": {
								"type" : "string"
							}
						},
						"port" : {
							"type" : "number",
							"minimum" : 1
						},
						"remote" : {
							"type" : "boolean"
						}
					},
					"required" : [
						"addrs", "port", "remote"
					]
				},
				"mock": {
					"type": "object",
					"additionalProperties": false,
					"properties": {
						"capacity": {
							"type": "integer",
							"minimum" : 0
						},
						"container": {
							"type": "object",
							"properties": {
								"capacity": {
									"type": "integer",
									"minimum" : 0
								},
								"countLimit": {
									"type": "integer",
									"minimum" : 0
								}
							},
							"required": [
								"capacity", "countLimit"
							]
						},
						"fail" : {
							"type" : "object",
							"additionalProperties" : false,
							"properties" : {
								"connections" : {
									"type" : "integer",
									"minimum" : 0
								},
								"responses" : {
									"type" : "integer",
									"minimum" : 0
								}
							},
							"required" : [
								"connections", "responses"
							]
						},
						"node" : {
							"type" : "boolean"
						}
					},
					"required": [
						"capacity", "container", "fail", "node"
					]
				},
				"net" : {
					"type" : "object",
					"additionalProperties" : false,
					"properties" : {
						"bindBacklogSize": {
							"type": "integer",
							"minimum" : 0
						},
						"interestOpQueued": {
							"type": "boolean"
						},
						"keepAlive": {
							"type": "boolean"
						},
						"linger": {
							"type": "integer",
							"minimum" : 0
						},
						"rcvBuf" : {
							"oneOf" : [
								{
									"type" : "integer",
									"minimum" : 0
								},
								{
									"type" : "string",
									"pattern" : "[\\d\\.]+[KMGTPE]?B?"
								}
							]
						},
						"reuseAddr": {
							"type": "boolean"
						},
						"sndBuf" : {
							"oneOf" : [
								{
									"type" : "integer",
									"minimum" : 0
								},
								{
									"type" : "string",
									"pattern" : "[\\d\\.]+[KMGTPE]?B?"
								}
							]
						},
						"tcpNoDelay": {
							"type": "boolean"
						},
						"timeoutMilliSec": {
							"type": "integer",
							"minimum" : 0
						},
						"ssl" : {
							"type" : "boolean"
						},
						"http": {
							"type": "object",
							"additionalProperties": false,
							"properties": {
								"api": {
									"type": "string",
									"pattern" : "[\\w\\d]+"
								},
								"fsAccess": {
									"type": "boolean"
								},
								"headers": {
									"type": "object",
									"properties": {
									}
								},
								"namespace": {
									"type": [
										"null",
										"string"
									]
								},
								"versioning": {
									"type": "boolean"
								}
							},
							"required": [
								"api", "fsAccess", "headers", "namespace", "versioning"
							]
						},
						"node" : {
							"type" : "object",
							"additionalProperties" : false,
							"properties" : {
								"addrs": {
									"type": "array",
									"items": {
										"type" : "string"
									}
								},
								"port" : {
									"type" : "number",
									"minimum" : 1
								}
							},
							"required" : [
								"addrs", "port"
							]
						}
					},
					"required" : [
						"bindBacklogSize", "interestOpQueued", "keepAlive", "linger",
						"rcvBuf", "reuseAddr", "sndBuf", "tcpNoDelay", "timeoutMilliSec", "ssl",
						"http", "node"
					]
				},
				"type": {
					"type": "string",
					"enum" : [
						"fs", "http"
					]
				}
			},
			"required": [
				"auth", "driver", "mock", "type"
			]
		},
		"version" : {
			"type" : "string"
		}
	},
	"required": [
		"item",
		"load",
		"scenario",
		"storage",
		"version"
	]
}
